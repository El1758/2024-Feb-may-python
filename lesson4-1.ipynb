   "source": [
    "#求直角三角形的角度\n",
    "import math\n",
    "side = eval(input('請輸入斜邊'))\n",
    "another = eval(input('請輸入對邊'))\n",
    "radian = math.asin(another/side)\n",
    "degree = math.degrees(radian)\n",
    "\n",
    "print(f'斜邊:{side},對邊:{another}')\n",
    "print(f'三角形的角度是{degree:.2f}')"
   ]
  }
